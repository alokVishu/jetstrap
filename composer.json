{
  "name": "alok-vishu/jetstrap",
  "description": "A Laravel 8 package to easily switch TailwindCSS resources generated by Laravel Jetstream to Bootstrap 4 or 5.",
  "keywords": ["laravel", "bootstrap", "jetstream"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alok Vishu",
      "email": "alokpixinvent@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0",
    "laravel/jetstream": "^2.3"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "AlokVishu\\Jetstrap\\": "src/AlokVishu/Jetstrap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlokVishu\\Jetstrap\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlokVishu\\Jetstrap\\JetstrapServiceProvider"
      ],
      "aliases": {
        "Jetstrap": "AlokVishu\\Jetstrap\\JetstrapFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "tests": "vendor/bin/phpunit",
    "testsVerbose": "@tests -vvv"
  }
}
